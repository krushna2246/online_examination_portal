<%- include("navbar.ejs") %>

<form action="/admin/save_subject" method="post">
    <div class="container-fluid mt-3 bg-white">
        <div class="row m-0">
            <div class="col-md-12">
                <h1 class="text-center">Add New Subject</h1>
            </div>
            <div class="col-md-12 mt-3">
                <h5 class="mt-3">Enter Subject</h5>
                <input type="text" placeholder="Enter subject name" name="subject_name" class="form-control" 
                required>
            </div>
            <div class="col-md-12 mt-1">
                <h5 class="mt-3">Select Subject</h5>
                <select name="year" class="form-control custom-select" >
                <option disabled selected value="">Select Option</option>
                <option value="FYBCS">FYBCS</option>
                <option value="SYBCS">SYBCS</option>
                <option value="TYBCS">TYBCS</option>
                <option value="FYMCS">FYMCS</option>
                <option value="SYMCS">SYMCS</option>
            </select>
            </div>
            
            <div class="col-md-12 text-center p-3">
                <button class="btn btn-dark">Add Subject</button>
            </div>
        </div>
    </div>
</form>




<%- include("footer.ejs") %>